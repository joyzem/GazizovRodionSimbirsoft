<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.AndroidPractice.Button">
        <item name="backgroundColor">@color/leaf</item>
        <item name="android:insetTop">2dp</item>
        <item name="android:insetBottom">2dp</item>
        <item name="android:textAppearance">@style/TextStyle7</item>
    </style>

    <style name="Theme.AndroidPractice.OutlinedButton">
        <item name="android:textAppearance">@style/TextStylePopupButton</item>
        <item name="android:letterSpacing">0.5</item>
    </style>

    <style name="Theme.AndroidPractice.Switch" parent="Widget.AppCompat.CompoundButton.Switch">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Switch</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="ThemeOverlay.App.Switch" parent="">
        <item name="colorSecondary">@color/leaf</item>
    </style>
</resources>